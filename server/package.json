    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "firebase-admin": "^7.3.0",
    "imagemagick": "^0.1.3",
    "mongodb": "^3.2.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "muter": "^0.7.0",
    "nodemailer": "^6.1.1",
